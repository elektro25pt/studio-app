{
  "version": 2,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/validation-key.txt",
      "dest": "/validation-key.txt",
      "headers": {
        "Content-Type": "text/plain"
      }
    },
    {
      "src": "/.well-known/pi.json",
      "dest": "/.well-known/pi.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/Terms-of-Service.pdf",
      "dest": "/Terms-of-Service.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "attachment; filename=\"Terms-of-Service.pdf\""
      }
    },
    {
      "src": "/(?!.*\\.(?:png|jpg|jpeg|gif|svg|ico|webp|woff2?|ttf|css|js|pdf|mp4|webm|ogg)$).*",
      "dest": "/index.html"
    }
  ]
}
